{
  "multipart": [
    {
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "structurize:spruce_paperwall/blockPaperwall_noside",
        "y": 270
      }
    }
  ]
}

